	<ThingDef Name="ResourceClay" ParentName="Resource" Abstract="True">
		<statBases>
			<Flammability>0</Flammability>
		</statBases>
		<thingCategories>
			<li>Clay</li>
		</thingCategories>
		<graphicData>
			<texPath>Things/Items/Resources/ClumpClay</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
	</ThingDef>
	
	<!-- Clay Blocks -->

