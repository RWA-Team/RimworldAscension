<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- 
	statBases - base stat values for the thing
	default values:
		- Flammability = 0
		- MaxHitPoints = 100
		
	stuffProps:	
		statOffsets - stuff based offset values
		statFactors - stuff based multiplier values
		stuffCommonality - weight chance to appear in trader stock	
	
	Work only in statBases:
		Blunt\SharpDamageMultiplier
	-->

	<ThingDef ParentName="ResourceOrganic">
		<defName>BarkFibre</defName>
		<label>bark fibre</label>
		<description>A bunch of bark fibre.</description>
		<graphicData>
			<texPath>Things/Items/Resources/BarkFibre</texPath>
			<color>(133,97,67)</color>
		</graphicData>
		<soundInteract>Grain_Drop</soundInteract>
		<soundDrop>Grain_Drop</soundDrop>
		<thingCategories>
			<li>ResourcesRaw</li>
		</thingCategories>
		<tradeability>Never</tradeability>
	</ThingDef>

	<ThingDef ParentName="ResourceOrganic">
		<defName>Xerigium</defName>
		<label>xerigium</label>
		<description>This plant was selectively bred for centuries by settlers. It is sometimes also called &quot;healer's hand&quot; by tribespeople - a nickname it owes to its five major medicinal virtues.</description>
		<thingClass>Medicine</thingClass>
		<graphicData>
			<texPath>Things/Plant/Healroot</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<MarketValue>5</MarketValue>
			<MedicalPotency>0.75</MedicalPotency>
			<DeteriorationRate>5</DeteriorationRate>
		</statBases>
		<thingCategories>
			<li>Medicine</li>
		</thingCategories>
		<tickerType>Rare</tickerType>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>150</daysToRotStart>
			</li>
		</comps>
	</ThingDef>

