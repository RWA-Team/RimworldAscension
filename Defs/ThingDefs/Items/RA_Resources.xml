<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- 
	statBases - initial stat values for the thing
	
	stuffProps:	
		statOffsets - stuff based offset values
		statFactors - stuff based multiplier values
		stuffCommonality - weight chance to appear in trader stock	
	
	Work only in statBases:
		Blunt\SharpDamageMultiplier
	-->

	<ThingDef Abstract="True" Name="ResourceBase" >
		<thingClass>ThingWithComps</thingClass>
			<label>unspecified resource</label>
		<category>Item</category>
		<resourceReadoutPriority>Middle</resourceReadoutPriority>
			<useHitPoints>true</useHitPoints>
		<selectable>true</selectable>
		<altitudeLayer>Item</altitudeLayer>
		<stackLimit>75</stackLimit>
		<comps>
			<li>
				<compClass>CompForbiddable</compClass>
			</li>
		</comps>
		<alwaysHaulable>true</alwaysHaulable>
		<drawGUIOverlay>true</drawGUIOverlay>
		<rotatable>false</rotatable>
		<pathCost>15</pathCost>
	</ThingDef>
	
	<ThingDef Abstract="True" Name="OrganicProductBase" ParentName="ResourceBase">
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>5</DeteriorationRate>
		</statBases>
		<tickerType>Rare</tickerType>
	</ThingDef>

	
	<!--===================== Plant Resources =====================-->


	<ThingDef ParentName="OrganicProductBase">
		<defName>Flax</defName>
		<label>flax</label>
		<description>A bunch of flax, a raw material used in linen manufacturing. On their own these are quite useless, but drying and fermentation of these plant products turns them into more valuable resources.</description>
		<graphicData>
			<texPath>Things/Items/Resources/Flax</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stackLimit>200</stackLimit>
		<soundInteract>Grain_Drop</soundInteract>
		<soundDrop>Grain_Drop</soundDrop>
		<thingCategories>
			<li>PlantFibres</li>
		</thingCategories>
		<statBases>
			<MarketValue>0.4</MarketValue>
		</statBases>
		<tradeability>Never</tradeability>
		<techLevel>Neolithic</techLevel>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>60</daysToRotStart>
			</li>
			<li Class="RA.CompProperties_Dryable">
				<daysToDry>7</daysToDry>
				<dryProducts>
					<li>
						<defName>BastFibre</defName>
						<conversionFactor>0.5</conversionFactor>
						<carryRotProgress>True</carryRotProgress>
					</li>
					<li>
						<defName>Linen</defName>
						<conversionFactor>0.5</conversionFactor>
						<carryRotProgress>True</carryRotProgress>
					</li>
				</dryProducts>
				<dryTemperature>
					<min>5</min>
					<max>25</max>
				</dryTemperature>
			</li>
		</comps>
		<!--Flax -[drying]-> Dried flax -[weaving]-> Linen
			CompProperties_Dryable -->
	</ThingDef>
	
	
	<ThingDef ParentName="ResourceBase">
		<defName>BastFibre</defName>
		<label>bast fibre</label>
		<description>A bunch of bast fibre.</description>
		<graphicData>
			<texPath>Things/Items/Resources/BastFibre</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(133,97,67)</color>
		</graphicData>
		<stackLimit>200</stackLimit>
		<soundInteract>Grain_Drop</soundInteract>
		<soundDrop>Grain_Drop</soundDrop>
		<thingCategories>
			<li>PlantFibres</li>
		</thingCategories>
		<statBases>
			<MarketValue>0.2</MarketValue>
		</statBases>
		<!--BastFibre -[processing]-> Product-->
	</ThingDef>
	
	
	<ThingDef ParentName="OrganicProductBase">
		<defName>Grass</defName>
		<label>grass</label>
		<description>A bunch of grass used for feeding animals and desperate humans. Not tradable as-is due to its commonality though it can be dried into more valuable and actually workable resources.</description>
		<graphicData>
			<texPath>Things/Items/Resources/Grass</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<stackLimit>200</stackLimit>
		<soundInteract>Grain_Drop</soundInteract>
		<soundDrop>Grain_Drop</soundDrop>
		<thingCategories>
			<li>PlantFibres</li>
			<li>PlantFoodRaw</li>
		</thingCategories>
		<statBases>
			<MarketValue>0.05</MarketValue>
		</statBases>
		<!--Grass -[drying]-> Hay-->
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>90</daysToRotStart>
			</li>
			<li Class="RA.CompProperties_Dryable">
				<daysToDry>7</daysToDry>
				<dryProducts>
					<li>
						<defName>Hay</defName>
						<carryRotProgress>True</carryRotProgress>
					</li>
				</dryProducts>
				<dryTemperature>
					<min>5</min>
					<max>30</max>
				</dryTemperature>
			</li>
		</comps>
		<ingestible>
			<preferability>Raw</preferability>
			<nutrition>0.07</nutrition>
			<eatEffect>EatVegetarian</eatEffect>
			<soundEat>RawVegetable_Eat</soundEat>
		</ingestible>
		<tradeability>Never</tradeability>
		<techLevel>Neolithic</techLevel>
	</ThingDef>
	
	
	<ThingDef ParentName="OrganicProductBase">
		<defName>Hay</defName>
		<label>hay</label>
		<description>A bunch of hay.</description>
		<graphicData>
			<texPath>Things/Items/Resources/Hay</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<stackLimit>200</stackLimit>
		<soundInteract>Grain_Drop</soundInteract>
		<soundDrop>Grain_Drop</soundDrop>
		<thingCategories>
			<li>PlantFibres</li>
			<li>PlantFoodRaw</li>
		</thingCategories>
		<statBases>
			<MarketValue>0.1</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>90</daysToRotStart>
			</li>
		</comps>
		<ingestible>
			<preferability>Raw</preferability>
			<nutrition>0.04</nutrition>
			<eatEffect>EatVegetarian</eatEffect>
			<soundEat>RawVegetable_Eat</soundEat>
		</ingestible>
	</ThingDef>
	
	
	<ThingDef ParentName="ResourceBase">
		<defName>RawCotton</defName>
		<label>raw cotton</label>
		<description>A bunch of raw cotton.</description>
		<graphicData>
			<texPath>Things/Items/Resources/Cotton</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stackLimit>200</stackLimit>
		<soundInteract>Grain_Drop</soundInteract>
		<soundDrop>Grain_Drop</soundDrop>
		<thingCategories>
			<li>PlantFibres</li>
		</thingCategories>
		<statBases>
			<MarketValue>0.5</MarketValue>
		</statBases>
		<!--Raw cotton -[weaving]-> Cloth-->
	</ThingDef>
	
	
	<ThingDef ParentName="ResourceBase">
		<defName>LeafBundle</defName>
		<label>leaf bundle</label>
		<description>A bundle of leaves.</description>
		<graphicData>
			<texPath>Things/Items/Resources/LeafBundle</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Grain_Drop</soundInteract>
		<soundDrop>Grain_Drop</soundDrop>
		<thingCategories>
			<li>ResourcesRaw</li>
		</thingCategories>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>10</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
	</ThingDef>


	<!--===================== Animal Resources =====================-->


	<ThingDef ParentName="ResourceBase">
		<defName>Bone</defName>
		<label>bone</label>
		<description>Primitive crafting material.</description>
		<graphicData>
			<texPath>Things/Items/Resources/Bone</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<soundInteract>Standard_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<thingCategories>
			<li>ResourcesRaw</li>
		</thingCategories>
		<statBases>
			<MarketValue>5</MarketValue>
			<MaxHitPoints>150</MaxHitPoints>
			<Flammability>0.5</Flammability>
			<SharpDamageMultiplier>1.2</SharpDamageMultiplier>
			<BluntDamageMultiplier>0.8</BluntDamageMultiplier>
		</statBases>
		<stuffProps>
			<categories>
				<li>Bone</li>
			</categories>
			<color>(216,220,216)</color>
			<soundImpactStuff>BulletImpactWood</soundImpactStuff>
			<soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
			<soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
			<smeltable>false</smeltable>
			<statOffsets>
				<Beauty>2</Beauty>
			</statOffsets>
			<statFactors>
				<MaxHitPoints>1.2</MaxHitPoints>
				<Flammability>0.5</Flammability>
				<WorkToMake>0.8</WorkToMake>
				<MarketValue>1.2</MarketValue>
				<Comfort>1.2</Comfort>
				<Beauty>1.2</Beauty>
				<TrapMeleeDamage>1.2</TrapMeleeDamage>
				<MeleeWeapon_Cooldown>0.8</MeleeWeapon_Cooldown>
			</statFactors>
		</stuffProps>
	</ThingDef>


	<!--===================== Stone Chunks =====================-->


	<ThingDef Name="ChunkBase" Abstract="True">
		<category>Item</category>
		<thingClass>ThingWithComps</thingClass>
		<resourceReadoutPriority>Middle</resourceReadoutPriority>
		<alwaysHaulable>true</alwaysHaulable>
		<altitudeLayer>Item</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.4</fillPercent>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<Flammability>0</Flammability>
			<Beauty>-10</Beauty>
			<Cleanliness>-2</Cleanliness>
		</statBases>
		<selectable>true</selectable>
		<pathCost>55</pathCost>
		<randomizeRotationOnSpawn>true</randomizeRotationOnSpawn>
		<saveCompressible>true</saveCompressible>
		<tradeability>Never</tradeability>
		<soundDrop>ChunkRock_Drop</soundDrop>
		<comps>
			<li>
				<compClass>CompForbiddable</compClass>
			</li>
		</comps>
		<thingCategories>
			<li>StoneChunks</li>
		</thingCategories>
		<stuffProps>
			<categories>
				<li>StonyRaw</li>
			</categories>
			<soundImpactStuff>BulletImpactGround</soundImpactStuff>
			<soundMeleeHitSharp>MeleeHit_Stone</soundMeleeHitSharp>
			<soundMeleeHitBlunt>MeleeHit_Stone</soundMeleeHitBlunt>
			<statOffsets>
				<Beauty>-10</Beauty>
				<Cleanliness>-2</Cleanliness>
			</statOffsets>
		</stuffProps>
	</ThingDef>	

	<ThingDef ParentName="ChunkBase">
		<defName>ChunkSandstone</defName>
		<label>sandstone chunk</label>
		<graphicData>
			<texPath>Things/Item/Chunk/ChunkStone</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<color>(126,104,94)</color>
		</graphicData>
		<butcherProducts>
			<BlocksSandstone>20</BlocksSandstone>
		</butcherProducts>
		<stuffProps>
			<color>(126,104,94)</color>
			<stuffAdjective>sandstone</stuffAdjective>
		</stuffProps>
