<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- 
	statBases - base stat values for the thing
	default values:
		- Flammability = 0
		- MaxHitPoints = 100
		
	stuffProps:	
		statOffsets - stuff based offset values
		statFactors - stuff based multiplier values
		stuffCommonality - weight chance to appear in trader stock	
	
	Work only in statBases:
		Blunt\SharpDamageMultiplier
	-->

	<ThingDef ParentName="ResourceOrganic">
		<defName>Bone</defName>
		<label>bone</label>
		<description>Primitive crafting material.</description>
		<graphicData>
			<texPath>Things/Items/Resources/Bone</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
			<color>(216,220,216)</color>
		</graphicData>
		<thingCategories>
			<li>ResourcesProcessed</li>
		</thingCategories>
		<statBases>
			<MarketValue>5</MarketValue>
			<SharpDamageMultiplier>1.2</SharpDamageMultiplier>
			<BluntDamageMultiplier>0.8</BluntDamageMultiplier>
		</statBases>
		<stuffProps>
			<categories>
				<li>Bone</li>
			</categories>
			<color>(216,220,216)</color>
			<soundImpactStuff>BulletImpactWood</soundImpactStuff>
			<soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
			<soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
			<smeltable>false</smeltable>
			<statOffsets>
				<Beauty>3</Beauty>
			</statOffsets>
			<statFactors>
				<Flammability>0.5</Flammability>
				<MaxHitPoints>0.8</MaxHitPoints>
				<WorkToMake>0.8</WorkToMake>
				<MarketValue>1.2</MarketValue>
				<Comfort>1.2</Comfort>
				<Beauty>1.2</Beauty>
				<TrapMeleeDamage>1.2</TrapMeleeDamage>
				<MeleeWeapon_Cooldown>0.8</MeleeWeapon_Cooldown>
			</statFactors>
		</stuffProps>
	</ThingDef>


	<!--===================== Wood =====================-->


	<ThingDef Name="ResourceWood" ParentName="ResourceOrganic" Abstract="True">
		<soundInteract>Wood_Drop</soundInteract>
		<soundDrop>Wood_Drop</soundDrop>
		<statBases>
			<DeteriorationRate>2</DeteriorationRate>
			<SharpDamageMultiplier>0.5</SharpDamageMultiplier>
			<MaxBurningTempCelsius>500</MaxBurningTempCelsius>
			<BurnDurationHours>1</BurnDurationHours>
		</statBases>
		<stuffProps>
			<stuffAdjective>wooden</stuffAdjective>
			<categories>
				<li>Wood</li>
			</categories>
			<color>(133,97,67)</color>
			<constructEffect>ConstructWood</constructEffect>
			<soundImpactStuff>BulletImpactWood</soundImpactStuff>
			<soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
			<soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
			<statOffsets>
				<Beauty>2</Beauty>
			</statOffsets>
			<statFactors>
				<MaxHitPoints>1</MaxHitPoints>
				<Beauty>1</Beauty>
				<Flammability>1</Flammability>
				<WorkToMake>1</WorkToMake>
				<DoorOpenSpeed>1</DoorOpenSpeed>
				<MeleeWeapon_Cooldown>1</MeleeWeapon_Cooldown>
			</statFactors>
		</stuffProps>
	</ThingDef>


	<ThingDef ParentName="ResourceWood">
		<defName>WoodLog</defName>
		<label>wood</label>
		<description>Wood from trees or other fibrous plants. Useful for building structures, weapons, and even simple prosthetics. A raw stick of wood can be used as a weapon in a pinch.</description>
		<graphicData>
			<texPath>Things/Item/Resource/WoodLog</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<thingCategories>
			<li>ResourcesRaw</li>
		</thingCategories>
		<statBases>
			<MarketValue>1.4</MarketValue>
			<MaxHitPoints>120</MaxHitPoints>
		</statBases>
		<stuffProps>
			<categories>
				<li>WoodLog</li>
			</categories>
		</stuffProps>
		<isBodyPartOrImplant>true</isBodyPartOrImplant>
		<techHediffsTags>
			<li>Poor</li>
		</techHediffsTags>
	</ThingDef>


	<!--===================== Stone =====================-->


	<ThingDef Name="ResourceStone" ParentName="Resource" Abstract="True">
		<statBases>
			<Flammability>0</Flammability>
			<BluntDamageMultiplier>1.25</BluntDamageMultiplier>
		</statBases>
		<soundInteract>Stone_Drop</soundInteract>
		<soundDrop>Stone_Drop</soundDrop>
		<stuffProps>
			<categories>
				<li>Stone</li>
			</categories>
			<soundImpactStuff>BulletImpactGround</soundImpactStuff>
			<soundMeleeHitSharp>MeleeHit_Stone</soundMeleeHitSharp>
			<soundMeleeHitBlunt>MeleeHit_Stone</soundMeleeHitBlunt>
			<statFactors>
				<Flammability>0.3</Flammability>
				<MaxHitPoints>1.5</MaxHitPoints>
				<WorkToMake>2.5</WorkToMake>
				<MarketValue>1.2</MarketValue>
				<Comfort>0.5</Comfort>
				<Beauty>0.8</Beauty>
				<DoorOpenSpeed>0.7</DoorOpenSpeed>
				<MeleeWeapon_Cooldown>1.2</MeleeWeapon_Cooldown>
				<TrapMeleeDamage>1.5</TrapMeleeDamage>
			</statFactors>
		</stuffProps>
	</ThingDef>

	<!--======== StoneChunks ========-->

	<ThingDef Name="ResourceStoneChunk" ParentName="ResourceStone" Abstract="True">
		<graphicData>
			<texPath>Things/Item/Chunk/ChunkStone</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<Beauty>-10</Beauty>
			<Cleanliness>-2</Cleanliness>
		</statBases>
		<fillPercent>0.4</fillPercent>
		<soundDrop>ChunkRock_Drop</soundDrop>
		<passability>PassThroughOnly</passability>
		<pathCost>55</pathCost>
		<randomizeRotationOnSpawn>true</randomizeRotationOnSpawn>
		<designateHaulable>true</designateHaulable>
		<saveCompressible>true</saveCompressible>
		<stackLimit>1</stackLimit>
		<tradeability>Never</tradeability>
		<thingCategories>
			<li>StoneChunks</li>
		</thingCategories>
		<stuffProps>
			<categories>
				<li>StoneChunk</li>
			</categories>
			<statOffsets>
				<Beauty>-10</Beauty>
				<Cleanliness>-2</Cleanliness>
			</statOffsets>
		</stuffProps>
	</ThingDef>

